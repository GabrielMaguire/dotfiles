[core]
	editor = nvim
[init]
	defaultBranch = main
[alias]
	s = status
	l = log --oneline
	unstage = restore --staged
	amend = commit --amend --no-edit
	pushf = push --force-with-lease
[rebase]
	updateRefs = true
	autoStash = true
[pull]
	rebase = true
